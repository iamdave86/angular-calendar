    "@angular/compiler-cli": "^19.0.4",
    "@types/jasmine": "^5.1.5",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.0.0",

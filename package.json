{
  "name": "angular-calendar",
  "description": "Angular Calendar app with reminders and weather integration",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamdave86/angular-calendar.git"
  },
  "author": "iamdave86 (https://github.com/iamdave86)",
  "keywords": [
    "javascript",
    "angular",
    "angular19",
    "angular-material",
    "typescript",
    "rxjs",
    "calendar",
    "reminder",
    "openweather",
    "openweatherapi",
    "openweather-api",
    "openweathermap",
    "openweathermapapi",
    "openweathermap-api",
    "reminder-application"
  ],
  "scripts": {
    "ng": "ng",
    "config": "ts-node src/environments/set-env.ts",
    "start": "npm run config && ng serve",
    "build": "NODE_ENV=production npm run config && ng build",
    "watch": "npm run config && ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
